{
  "manifest_version": 3,
  "name": "Telegram Video Downloader",
  "version": "2.1.1",
  "description": "Download videos from Telegram Web (K and A versions)",
  "permissions": ["scripting"],
  "host_permissions": ["https://web.telegram.org/*"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
